{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    { "source": "/config", "destination": "/api/config" },
    { "source": "/create-checkout-session", "destination": "/api/create-checkout-session" }
  ]
}
